{
    "comments": {
        "lineComment": "//",
        "blockComment": [ "/*", "*/" ]
    },
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"]
    ],
    "autoClosingPairs": [
      { "open": "{", "close": "}" },
      { "open": "[", "close": "]" },
      { "open": "(", "close": ")" }
    ],
    "surroundingPairs": [
      ["{", "}"],
      ["[", "]"],
      ["(", ")"],
      ["'", "'"],
      ["\"", "\""]
    ],
    "folding": {
        "markers": 
        {
          "start": "^\\s*(SUB|Sub|sub|IF|If|if|FOR|For|for|DO|Do|do|SWITCH|Switch|switch)(?!\\s*\\()\\b",
          "end": "^\\s*(NEXT|Next|next|LOOP|Loop|loop)|^\\s*(END|End|end)\\s*(SUB|Sub|sub|IF|If|if|SWITCH|Switch|switch)\\b"
        }
      }
}